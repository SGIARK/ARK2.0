{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "object": {
      "type": "string"
    },
    "created": {
      "type": "integer"
    },
    "model": {
      "type": "string"
    },
    "choices": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "index": {
            "type": "integer"
          },
          "message": {
            "type": "object",
            "properties": {
              "role": {
                "type": "string"
              },
              "content": {
                "type": "string"
              }
            },
            "required": [
              "role",
              "content"
            ]
          },
          "finish_reason": {
            "type": [
              "null",
              "string"
            ]
          }
        },
        "required": [
          "index",
          "message",
          "finish_reason"
        ]
      }
    }
  },
  "required": [
    "id",
    "object",
    "created",
    "model",
    "choices"
  ]
}